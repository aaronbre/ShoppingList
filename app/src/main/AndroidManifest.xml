<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aaronbrecher.shoppinglist">

    <application
        android:name=".ShoppingListApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".listactivity.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".addList.NewListActivity"
            android:label="New Shopping List"
            android:parentActivityName=".listactivity.ListActivity" />
        <activity android:name=".listdetail.ListDetailActivity" />
        <activity android:name=".itemdetail.ItemDetailActivity"></activity>
    </application>

</manifest>